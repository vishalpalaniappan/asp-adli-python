{
    "description": "Contains the design traces as constructed through the functional, selector and behavioral abstsractions. This will allow the construction of design traces across boundaries such as threads, nodes and processes.",
    "modules": [
        {
            "name":"root",
            "nodes": [
                {
                    "id": "CreateBasketAndBookShelf"
                },
                {
                    "id": "DisplayMenuAndAcceptResponse"                    
                },
                {
                    "type": "selector",
                    "description": "Select Menu Option",
                    "selectors": [
                        {
                            "id": "AcceptBookAndAddToBasket",
                            "behavior": [
                                {
                                    "id": "AcceptBookInformation",
                                    "module_name": "AcceptBookModule",
                                    "type":"module"
                                },
                                {
                                    "id": "AddBookToBasket"
                                }
                            ]
                        },
                        {
                            "id": "PlaceBooksFromBasketOntoShelf",
                            "behavior":  [
                                {
                                    "id": "PlaceBookOnShelf",
                                    "type":"module"
                                },
                                {
                                    "id": "AddBookToBasket"
                                }
                            ]
                        },
                        {
                            "id": "DisplayingContentsOfLibrary",
                            "behavior": [
                                {
                                    "id": "DisplayBookShelf"
                                },
                                {
                                    "id": "DisplayBasket"
                                },
                                {
                                    "id": "AddBookToBasket"
                                }
                            ]
                        },
                        {
                            "id": "LeaveLibrary",
                            "type": "end"
                        }
                    ]
                },
                {
                    "type":"goto",
                    "module": "root",
                    "target_id": "DisplayMenuAndAcceptResponse"
                }
            ]
        },
        {
            "name":"AcceptBookModule",
            "nodes": [
                {
                    "id": "DisplayRequest"
                },
                {
                    "id": "AcceptBookName"
                },
                {
                    "id": "AcceptBookGenre"
                },
                {
                    "id": "AssembleBookDetails"
                },
                {
                    "id": "ReturnBookDetails"
                }
            ]
        },
        {
            "name":"PlaceBookOnShelf",
            "nodes": [
                {
                    "type": "selector",
                    "id": "CheckingBookForBasket",
                    "description": "Check for books in basket",
                    "selectors": [
                        {
                            "id": "PlacingBookOnShelf",
                            "behavior": [
                                {
                                    "id": "GettingBookFromBasket"
                                },
                                {
                                    "id": "GettingFirstNameOfBook"
                                },
                                {
                                    "type": "selector",
                                    "id": "CheckingIfBookHasName",
                                    "description": "Check if slot exists for book",
                                    "selectors": [
                                        {
                                            "id": "CreateSlotForBookOnShelf"
                                        },
                                        {
                                            "id": "DisplayConfirmationThatSlotExists"
                                        }
                                    ]
                                },
                                {
                                    "id": "AddingBookToShelf"
                                },
                                {
                                    "type":"goto",
                                    "module": "CheckingBookForBasket"
                                }
                            ]
                        },
                        {
                            "id": "ReturnBooks",
                            "type": "return"
                        }
                    ]
                }
            ]
        }
    ]
}